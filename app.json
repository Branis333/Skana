{
    "expo": {
        "name": "Skana",
        "slug": "skana-mobile",
        "version": "1.0.0",
        "orientation": "portrait",
        "userInterfaceStyle": "light",
        "platforms": [
            "ios",
            "android"
        ],
        "splash": {
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.brainink.skana"
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#ffffff"
            },
            "package": "com.brainink.skana"
        },
        "scheme": "skana"
    }
}